; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="SSCS"
config/version="0.1"
run/main_scene="uid://dxie0udrkwq8l"
config/use_custom_user_dir=true
config/custom_user_dir_name="SSCS"
config/features=PackedStringArray("4.4", "Forward Plus", "Redot")
run/delta_smoothing=false
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/show_image=false
boot_splash/use_filter=false
config/icon="res://assets/branding/icon.svg"

[audio]

buses/default_bus_layout="res://assets/defaults/default_bus_layout.tres"
driver/output_latency=5

[autoload]

SSCS="*res://scripts/singletons/SSCS.gd"
Terminal="*res://scenes/singletons/terminal.tscn"
CommandLoader="*res://scripts/singletons/CommandLoader.gd"
AudioManager="*res://scripts/singletons/AudioManager.gd"
SteamHandler="*res://scripts/singletons/SteamHandler.gd"

[debug]

settings/profiler/max_timestamp_query_elements=1024
gdscript/warnings/untyped_declaration=2

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/size/mode=2
window/stretch/aspect="ignore"
window/vsync/vsync_mode=0

[dotnet]

project/assembly_name="SSCS"

[editor_plugins]

enabled=PackedStringArray()

[file_customization]

folder_colors={
"res://assets/": "red",
"res://scenes/": "purple",
"res://scripts/": "blue"
}

[input]

pause={
"deadzone": 0.2,
"events": [Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":0,"window_id":0,"alt_pressed":false,"shift_pressed":false,"ctrl_pressed":false,"meta_pressed":false,"pressed":false,"keycode":0,"physical_keycode":32,"key_label":0,"unicode":0,"location":0,"echo":false,"script":null)
]
}
reset={
"deadzone": 0.2,
"events": [Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":-1,"window_id":0,"alt_pressed":false,"shift_pressed":false,"ctrl_pressed":false,"meta_pressed":false,"pressed":false,"keycode":0,"physical_keycode":82,"key_label":0,"unicode":114,"location":0,"echo":false,"script":null)
]
}

[input_devices]

buffering/agile_event_flushing=true

[network]

limits/debugger/max_queued_messages=4096

[physics]

